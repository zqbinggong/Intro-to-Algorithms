
public class ArraySorts {
	int i,j,q;
	int key;
	
	//插入排序
	public int[] InsertSort(int[] arr){
		for(i = 1; i < arr.length; i++){
			key = arr[i];
			j = i - 1;
			while(j>0 && arr[j] > key)
				arr[j+1] = arr[j++];
			arr[j+1] = key;
		}
		return arr;
	}
	
	//归并排序
	public int[] mergeSort(int[] arr, int p, int r){
		
		return arr;
		
	}
}
